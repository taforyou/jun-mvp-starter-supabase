{
  "name": "jun-mvp-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https -p 4416",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase db push",
    "supabase:functions:deploy": "supabase functions deploy",
    "supabase:functions:serve": "supabase functions serve",
    "dev:supabase": "concurrently \"npm run dev\" \"npm run supabase:start\"",
    "build:functions": "cd functions && npm run build",
    "emulators": "cd functions && npm run build:watch & cd .. & firebase emulators:start --import .firebase-local-data --export-on-exit",
    "dev:all": "concurrently \"npm run dev\" \"npm run emulators\"",
    "deploy:functions": "cd functions && npm run build && cd .. && firebase deploy --only functions",
    "test:deploy": "cd functions && npm run build && echo \"âœ… Functions build successful and ready for deployment\""
  },
  "dependencies": {
    "@line/liff": "2.25.1",
    "@radix-ui/react-avatar": "1.1.3",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-dropdown-menu": "2.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "@supabase/supabase-js": "^2.49.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "firebase-admin": "13.2.0",
    "lucide-react": "0.479.0",
    "next": "15.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "3.0.2",
    "tailwindcss-animate": "1.0.7",
    "tailwindcss-jun-layout": "0.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
